{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": ".",
    "paths": {
      "@redux-hook": ["redux/app/hook"],
      "@components/*": ["components/*", "stories/components/*"],
      // "@components/*": ["stories/components/*"],
      "@configs/*": ["configs/*"],
      "@enums/*": ["enums/*"],
      "@hooks/*": ["hooks/*"],
      "@redux/*": ["redux/store/slice/*"],
      "@helpers/*": ["helpers/*"],
      "@store/*": ["store/*"],
      "@utils/*": ["utils/*"],
      "@services/*": ["services/*"],
      "@request-providers/*": ["services/request/providers/*"],
      "@page-providers/*": ["services/page-providers/*"],
      "@views/*": ["views/*"],
      "@styles/*": ["styles/*"]
    }
  },
  "include": ["next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "**/*.jsx",
    "**/*.js"
  ],
  "exclude": ["node_modules"]
}